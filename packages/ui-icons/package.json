{
  "name": "@kaltura-path/ui-icons",
  "version": "0.0.0",
  "description": "",
  "author": "Kaltura",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaltura/path-design-system"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "reset": "npm run clean && rm -rf node_modules",
    "analyze": "npm run build && npx source-map-explorer dist/index.js",
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rimraf dist && rollup -c",
    "start": "rollup -c -w",
    "src:icons": "rimraf src && npm run src:icons:flat && npm run src:icons:twoTones && npm run src:icons:storybook",
    "src:icons:flat": "babel-node --config-file ./babel-builder.config.js builder/builder.js --output-dir ./src --svg-dir ./assets/svgs/flat --is-flat",
    "src:icons:twoTones": "babel-node --config-file ./babel-builder.config.js builder/builder.js --output-dir ./src --svg-dir ./assets/svgs/twoTone",
    "src:icons:storybook": "babel-node --config-file ./babel-builder.config.js builder/builder-story.js --output-dir ./src --icons-dir ./src"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "@path-composer/ui-icons": "file:../ui-icons"
  },
  "peerDependencies": {
    "antd": "3.25.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-jss": "^10.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
